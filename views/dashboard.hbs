  {{!-- Debt / payment --}}
  <div class="row">
    <div class="col-md-7 grid-margin stretch-card">
      <div class="card">
        <div class="card-body">
          <p class="card-title mb-0">Order</p>
          <div class="table-responsive">
            <table class="table table-striped table-borderless">
              <thead>
                <tr>
                  <th>ID</th>
                  <th>User ID</th>
                  <th>Order At</th>
                  <th>Status</th>
                </tr>
              </thead>
              <tbody>
                {{#each orders}}
                <tr>
                  <td>{{id}}</td>
                  <td class="font-weight-bold">${{user_id}}</td>
                  <td>{{order_at}}</td>
                  <td class="font-weight-medium"><div
                      class="badge badge-{{#ifStr status "Pending"}}warning{{else}}success{{/ifStr}}"
                    >{{status}}</div></td>
                </tr>
                {{/each}}
              </tbody>
            </table>
          </div>
        </div>
